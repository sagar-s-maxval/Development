    @AuraEnabled
    public static List<WrapperComboBoxOption> searchInventors(String searchWord, List<String> InvIds) {
        try {
            Id userId = UserInfo.getUserId();
            List<WrapperComboBoxOption> objOptions = new List<WrapperComboBoxOption>();
            searchWord = searchWord + '%';
            if (Schema.SObjectType.SymphonyIPM__Inventor__c.isAccessible() && SymphonyIPM__Inventor__c.SObjectType.getDescribe().isAccessible()) {
                List<SymphonyIPM__Inventor__c> lstInventors = [SELECT Id, Name,SymphonyIPM__Email__c, SymphonyIPM__RecordTypeName__c FROM SymphonyIPM__Inventor__c WHERE (Name LIKE :searchWord OR SymphonyIPM__Email__c LIKE :searchWord) AND Id NOT IN :InvIds AND SymphonyIPM__Employment_status__c = 'Active' ORDER BY Name LIMIT 100];
                for (SymphonyIPM__Inventor__c objInventors : lstInventors) {
                    WrapperComboBoxOption objOption = new WrapperComboBoxOption();
                    if (objInventors.SymphonyIPM__Email__c != null) {
                        objOption.label = objInventors.Name + ' (' + objInventors.SymphonyIPM__Email__c + ')';
                    } else {
                        objOption.label = objInventors.Name;
                    }
                    objOption.value = objInventors.Id;
                    objOptions.add(objOption);
                }
            }
            return objOptions;
        } catch (Exception exp) {
            throw exp;
        }
    }


    @AuraEnabled
    public static List<WrapperPill> getSelectedInventors(List<String> InvIds, Id primaryInventor) {
        System.debug(InvIds);
        Set<String> idSet = new Set<String>();
        idSet.addAll(InvIds);
        System.debug(idSet);
        try {
            List<WrapperPill> objPills = new List<WrapperPill>();
            List<SymphonyIPM__Inventor__c> lstInventors = [SELECT Id, Name,SymphonyIPM__Email__c FROM SymphonyIPM__Inventor__c WHERE Id IN :idSet AND Id != :primaryInventor ORDER BY Name];
            System.debug(lstInventors);
            for (SymphonyIPM__Inventor__c objInventor : lstInventors) {
                WrapperPill objPill = new WrapperPill();
                objPill.type = 'avatar';
                objPill.label = objInventor.Name;
                objPill.name = objInventor.Id;
                objPill.fallbackIconName = 'standard:user';
                objPill.variant = 'circle';
                if (objInventor.SymphonyIPM__Email__c != null) {
                    objPill.label = objInventor.Name + ' (' + objInventor.SymphonyIPM__Email__c + ')';
                } else {
                    objPill.label = objInventor.Name;
                }
                objPills.add(objPill);
            }
            return objPills;
        } catch (Exception exp) {
            throw exp;
        }
    }

    @AuraEnabled
    public static List<SymphonyIPM__Inventor__c> getSelectedInventorsToFlow(List<String> InvIds, Id primaryInventor) {
        try {
            return [SELECT Id, Name, SymphonyIPM__FirstName__c, SymphonyIPM__LastName__c, SymphonyIPM__Email__c FROM SymphonyIPM__Inventor__c WHERE Id IN :InvIds AND Id != :primaryInventor AND SymphonyIPM__Email__c != NULL ORDER BY Name];
        } catch (Exception exp) {
            throw exp;
        }
    }

    @AuraEnabled
    public static void deleteIDInventor(Id inventorId, Id inventionDisclosureId) {

        List<SymphonyIPM__Additional_Inventor__c> idInventors = [SELECT Id FROM SymphonyIPM__Additional_Inventor__c WHERE SymphonyIPM__Inventor__c = :inventorId AND SymphonyIPM__Invention_Disclosure_New__c = :inventionDisclosureId];
        Database.delete(idInventors);
    }

    @AuraEnabled
    public static WrapperPill fetchUser() {
        User objuser = [SELECT Id,Name, Email,FederationIdentifier FROM User WHERE Id = :UserInfo.getUserId()];
        System.debug('User id' + objuser.Id);
        SymphonyIPM__Inventor__c objinventor = [SELECT Id,Name,SymphonyIPM__Email__c,SymphonyIPM__RecordTypeName__c,SymphonyIPM__User__c FROM SymphonyIPM__Inventor__c WHERE SymphonyIPM__Email__c = :objuser.Email LIMIT 1];
        System.debug('Fetched Record' + objinventor);
        if (objinventor != null) {
            WrapperPill objPill = new WrapperPill();
            objPill.type = 'avatar';
            objPill.label = objinventor.Name;
            objPill.name = objinventor.Id;
            objPill.fallbackIconName = 'standard:user';
            objPill.variant = 'circle';
            if (objinventor.SymphonyIPM__Email__c != null) {
                objPill.label = objinventor.Name + ' (' + objinventor.SymphonyIPM__Email__c + ')';
            } else {
                objPill.label = objinventor.Name;
            }
            System.debug('Return value' + objPill);
            return objPill;
        } else {
            return null;
        }
    }

