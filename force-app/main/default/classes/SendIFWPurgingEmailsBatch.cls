public with sharing class SendIFWPurgingEmailsBatch implements Database.Batchable<SObject> {
    public Map<Id, List<SymphonyIPM__Hierarchy_Role__c>> hierarchyMap = new Map<Id, List<SymphonyIPM__Hierarchy_Role__c>>();

    public Iterable<SObject> start(Database.BatchableContext param1) {
        Date deadline1 = PatentService.getDeadlineFromToday('Send_IFW_First_Purging_Emails_Deadline');
        Date deadline2 = PatentService.getDeadlineFromToday('Send_IFW_Purging_Emails_Deadline');

        List<SymphonyIPM__Patent__c> patents = [
                SELECT Id, SymphonyIPM__Hierarchy__c, SymphonyIPM__Title_of_Invention__c, Name, SymphonyIPM__Issue_Date_of_Patent__c
                FROM SymphonyIPM__Patent__c
                WHERE (SymphonyIPM__Issue_Date_of_Patent__c = :deadline2 OR SymphonyIPM__Issue_Date_of_Patent__c = :deadline1)
                AND Litigation_on_hold__c = FALSE
        ];



        return patents;
    }

    public void execute(Database.BatchableContext param1, List<SObject> patents) {
        Set<Id> hierarchies = new Set<Id>();
        for (SymphonyIPM__Patent__c patent : (List<SymphonyIPM__Patent__c>)patents) {
            hierarchies.add(patent.SymphonyIPM__Hierarchy__c);
        }

        Map<Id, List<SymphonyIPM__Hierarchy_Role__c>> roleMap = new Map<Id, List<SymphonyIPM__Hierarchy_Role__c>>();
        for (SymphonyIPM__Hierarchy_Role__c role : [
                SELECT Id, SymphonyIPM__Hierarchy_Path__c, Person__r.SymphonyIPM__Email__c, Person__r.Name
                FROM SymphonyIPM__Hierarchy_Role__c
                WHERE SymphonyIPM__Hierarchy_Path__c IN :hierarchies
        ]) {
            List<SymphonyIPM__Hierarchy_Role__c> roles = new List<SymphonyIPM__Hierarchy_Role__c>();
            if (roleMap.containsKey(role.SymphonyIPM__Hierarchy_Path__c)) {
                roles.addAll(roleMap.get(role.SymphonyIPM__Hierarchy_Path__c));
            }
            roles.add(role);
            roleMap.put(role.SymphonyIPM__Hierarchy_Path__c, roles);
        }
        hierarchyMap = roleMap;

        List<Messaging.SingleEmailMessage> emails = new List<Messaging.SingleEmailMessage>();
        for (SymphonyIPM__Patent__c patent : (List<SymphonyIPM__Patent__c>) patents) {
            System.debug('INSIDE 1');
            System.debug(patent.SymphonyIPM__Hierarchy__c);
            System.debug(hierarchyMap);
            if (hierarchyMap.get(patent.SymphonyIPM__Hierarchy__c) != null && !hierarchyMap.get(patent.SymphonyIPM__Hierarchy__c).isEmpty()) {
                System.debug('INSIDE 2');
                for (SymphonyIPM__Hierarchy_Role__c role : hierarchyMap.get(patent.SymphonyIPM__Hierarchy__c)) {
                    System.debug('INSIDE');
                    if (role.Person__r.SymphonyIPM__Email__c != '') {
                        if (patent.SymphonyIPM__Issue_Date_of_Patent__c <= Date.today().addMonths(-3)) {
                            emails.add(composeEmailThreeMonths(
                                    role.Person__r.Name,
                                    role.Person__r.SymphonyIPM__Email__c,
                                    patent.SymphonyIPM__Title_of_Invention__c,
                                    patent.Name
                            )
                            );
                        } else {
                            emails.add(composeEmailHundredAndTenDays(
                                    role.Person__r.Name,
                                    role.Person__r.SymphonyIPM__Email__c,
                                    patent.SymphonyIPM__Title_of_Invention__c,
                                    patent.Name
                            )
                            );
                        }

                    }
                }
            }
        }
        System.debug(emails);
        Messaging.sendEmail(emails);
    }

    public void finish(Database.BatchableContext param1) {
    }

    private static Messaging.SingleEmailMessage composeEmailThreeMonths(String personName, String emailAddress, String title, String docketNo) {
        List<String> to = new List<String>();

        to.add(emailAddress);
        Messaging.SingleEmailMessage email = new Messaging.SingleEmailMessage();

        email.setToAddresses(to);
        email.setSubject('Documents purging notification');
        email.setHtmlBody('Hi ' + personName
                + '<br/> Docket No - ' + docketNo
                + '<br/> Title - ' + title
                + '<br/> This is to inform you that the documents <i>(Initial and intermediate versions)</i> related to this application will be purged in the next N days from today.<br/><br/>Regards<br/> Thermo Fisher');
        return email;
    }

    private static Messaging.SingleEmailMessage composeEmailHundredAndTenDays(String personName, String emailAddress, String title, String docketNo) {
        List<String> to = new List<String>();

        to.add(emailAddress);
        Messaging.SingleEmailMessage email = new Messaging.SingleEmailMessage();

        email.setToAddresses(to);
        email.setSubject('Documents purging notification');
        email.setHtmlBody('Hi ' + personName
                + '<br/> Docket No - ' + docketNo
                + '<br/> Title - ' + title
                + '<br/> This is to inform you that the documents <i>(Initial and intermediate versions)</i> related to this application will be purged in the next N days from today.<br/><br/>Regards<br/> Thermo Fisher');
        return email;
    }

