public with sharing class PatentRatingSelector {

    public static List<SymphonyIPM__IDF_Review__c> getReview(Id userId, Id patentId) {
        return [
                SELECT Id, Intensifying_Innovation__c, CSO_Program_Name__c, CSO_Innovative_Type__c, CSO_Top_Program__c, CSO_NPI_Number__c, CSO_Tech_Area_1__c, CSO_Tech_Area_2__c,
                        Patent_Product__c, Product_multiple__c, Product_Lines__c, Product_Line__c, Taxonomy__c, Case_Rating__c
                FROM SymphonyIPM__IDF_Review__c
                WHERE OwnerId = :userId AND SymphonyIPM__Base_Patent_Record__c = :patentId
        ];
    }

    public static List<SymphonyIPM__IDF_Review__c> getReviews(Id patentId) {
        return [
                SELECT Id, Intensifying_Innovation__c, CSO_Program_Name__c, CSO_Innovative_Type__c, CSO_Top_Program__c, CSO_NPI_Number__c, CSO_Tech_Area_1__c, CSO_Tech_Area_2__c,
                        Product__c, Product_multiple__c, Product_Line__c, Product_Lines__c, Taxonomy__c, CreatedBy.Name, Case_Rating__c, Profile_Name__c
                FROM SymphonyIPM__IDF_Review__c
                WHERE SymphonyIPM__Base_Patent_Record__c = :patentId
        ];
    }

    public static List<Patent_Product_Line__c> getSelectedLines(List<Id> selectedIds) {
        return [
                SELECT Id, Label__c
                FROM Patent_Product_Line__c
                WHERE Id IN :selectedIds
        ];
    }

    public static List<Patent_Product_Line__c> getSelectedLinesByLabels(List<String> selectedLabels) {
        return [
                SELECT Id, Label__c
                FROM Patent_Product_Line__c
                WHERE Label__c IN :selectedLabels
        ];
    }

    public static List<Patent_Product__c> getSelectedProductsByLabels(List<String> selectedLabels) {
        return [
                SELECT Id, Label__c
                FROM Patent_Product__c
                WHERE Label__c IN :selectedLabels
        ];
    }

    public static List<Patent_Product__c> getSelectedProducts(List<Id> selectedIds) {
        return [
                SELECT Id, Label__c
                FROM Patent_Product__c
                WHERE Id IN :selectedIds
        ];
    }

    public static List<Patent_Product__c> getPatentProducts(List<String> linesIds) {
        return [
                SELECT Id, Label__c
                FROM Patent_Product__c
                WHERE Patent_Product_Line__c IN :linesIds
                ORDER BY Label__c
        ];
    }

    public static List<Patent_Product_Line__c> getProductLines(List<Id> selectedIds) {
        return [
                SELECT Id, Label__c
                FROM Patent_Product_Line__c
                WHERE Id NOT IN :selectedIds
                ORDER BY Label__c
        ];
    }

    public static List<Patent_Product__c> searchProducts(String searchWord) {
        return [
                SELECT Id, Label__c
                FROM Patent_Product__c
                WHERE Label__c LIKE : searchWord
                ORDER BY Label__c
        ];
    }
