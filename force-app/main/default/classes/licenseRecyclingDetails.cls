    public static List<User> getUserList(String profileVal,Set<Id> usrIds) //sends the list of users to be deactivated to the batch class
    {
        List<user> deativatedUsers = new List<User>();
        if(Schema.sObjectType.Profile.isAccessible())
        {
        List<user> loginHistory = [SELECT id,Name,Email,Username,isActive,Profile.Name, LastLogindate FROM User WHERE isActive=true and LastLogindate!=null And Profile.Name=:ProfileVal];//:ProfileVal
        if(test.isRunningTest())
        {
            User u = new User(ProfileId = [SELECT Id FROM Profile WHERE Name = 'Platform Inventor'].Id,
         LastName = 'name',
         Email = 'usertestmail@testdata.com',
         Username = 'usertestmail1@testdata.com' + System.currentTimeMillis(),
         CompanyName = 'TEST',
         Title = 'title',
         Alias = 'alias',
         TimeZoneSidKey = 'America/Los_Angeles',
         EmailEncodingKey = 'UTF-8',
         LanguageLocaleKey = 'en_US',
         LocaleSidKey = 'en_US');
         loginHistory.add(u);
        }
        for(user u:loginHistory)
        {
            //for now if user is not active from some(Mention in criteria) days we are deactivating the user
            //&& (Integer.valueof(Licence_Recycling_JIT__c.getInstance(u.Profile.NAme).Deactivation_Criteria__c) != null)
            Integer diff = 0;
            integer criteria = 0;
            
            if(test.isRunningTest())
            {
                diff = 10;
                criteria = 5;
            }
            else
            {
                Licence_Recycling_JIT__c InvRecord = Licence_Recycling_JIT__c.getInstance('Platform Inventor');
                string CustomInvProval = InvRecord.Name;
                string ProfileName = u.Profile.Name;
                if(CustomInvProval == ProfileName)
                {
                    criteria = Integer.valueof(Licence_Recycling_JIT__c.getInstance(u.Profile.Name).Deactivation_Criteria__c);
                    diff = u.LastLogindate.Date().daysBetween(datetime.now().Date());
                }
            }   
            
            //if the last logged in date is greater than the deactivation criteria, then the users are deactivated
            if(diff > criteria && (!usrIds.contains(u.id)))
            {
                system.debug('Name is'+u.Name);
                u.isActive = false;
                deativatedUsers.add(u);
            }
        }
        }
        return deativatedUsers;
    }
    
