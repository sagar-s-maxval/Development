public with sharing class IDFSubmissionController {

    @AuraEnabled
    public static SymphonyIPM__Invention_Disclosure_New__c getInventionDisclosure(Id recordId) {
        return [
          SELECT Id, Name, SymphonyIPM__Title__c, SymphonyIPM__Primary_Inventor__c, SymphonyIPM__Inventors__c, Group__c, Division__c, Business_Unit__c
          FROM SymphonyIPM__Invention_Disclosure_New__c
          WHERE Id = :recordId
        ];
    }

    @AuraEnabled
    public static List<SymphonyIPM__Inventor__c> getInventors(Id inventionDisclosureId) {

        Set<Id> inventorsIDs = new Set<Id>();
        for (SymphonyIPM__Invention_Disclosure_New__c inventionDisclosure : [SELECT SymphonyIPM__Primary_Inventor__c FROM SymphonyIPM__Invention_Disclosure_New__c WHERE Id = :inventionDisclosureId]) {
            inventorsIDs.add(inventionDisclosure.SymphonyIPM__Primary_Inventor__c);
        }

        return [
                SELECT Name, SymphonyIPM__Address__c, SymphonyIPM__Citizenship__c, SymphonyIPM__Email__c
                FROM SymphonyIPM__Inventor__c
                WHERE Id IN : inventorsIDs
        ];
    }

    @AuraEnabled
    public static List<SymphonyIPM__Inventor__c> getSelectedInventors(List<Id> inventorsIDs) {

        return [
                SELECT Name, SymphonyIPM__Address__c, SymphonyIPM__Citizenship__c, SymphonyIPM__Email__c
                FROM SymphonyIPM__Inventor__c
                WHERE Id IN : inventorsIDs
        ];
    }

    @AuraEnabled
    public static List<SymphonyIPM__Inventor__c> getInventorsList(List<Id> inventorsIDs) {
        return [
                SELECT Id, Name, SymphonyIPM__Address__c, SymphonyIPM__Citizenship__c, SymphonyIPM__CitizenshipPicklist__c,
                        SymphonyIPM__Email__c, SymphonyIPM__Location__c, SymphonyIPM__RecordTypeName__c, PhoneNumber__c, EmployerAddress__c, EmployerName__c
                FROM SymphonyIPM__Inventor__c
                WHERE Id IN : inventorsIDs
        ];
    }

