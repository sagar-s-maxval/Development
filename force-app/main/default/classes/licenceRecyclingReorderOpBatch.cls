                htmlBodyReorder = htmlBodyReorder + '<table border="1">   <tr>       <th><b>License Type</b></th>        <th><b>Total Number of Licences</b></th>        <th><b>Number of Available Licenses</b></th>        <th><b>Used Licenses</b></th>       <th><b>Recycling Threshold(%)</b></th>      <th><b>Reorder Quantity</b></th>     </tr>   ';
                htmlBodyReorder = htmlBodyReorder + '<tr>     <td><b>' + instance.Licence_Type__c + '</b></td>        <td align="center"><b>' + instance.Total_Number_of_Licences__c + '</b></td>     <th><b>' + instance.Number_of_Available_Licenses__c + '</b></th>        <th><b>' + instance.Used_Licenses__c + '</b></th>       <th><b>' + instance.Usage_Recycling__c + '</b></th>     <th><b>' + instance.Reorder_Quantity__c + '</b></th>     </tr>    </table>';
                htmlBodyReorder = htmlBodyReorder + ' <br/> <br/> Regards, <br>  System Team ';
                Messaging.SingleEmailMessage seREmail = new Messaging.SingleEmailMessage();
                String[] sendingTo = new String[]{
                        instance.Re_order_email__c
