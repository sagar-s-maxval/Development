            If (deactivatedUsersFinal.size() > 0) {

                //create body of deactivated user which will share with Admin
                system.debug('deactivated users list size: ' + deativatedUsers.size());
                htmlBodyDeactivatedUser = 'Hi,<br/> Please refer the table for users who have been deactivated from your Org: ' + '' + UserInfo.getOrganizationName() + ' ' + 'having Org ID: ' + '' + UserInfo.getOrganizationId() + ' <br/><br/><br/>';
