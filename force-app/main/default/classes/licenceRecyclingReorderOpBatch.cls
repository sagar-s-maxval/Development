    global Database.QueryLocator start(Database.BatchableContext BC) {
        string query = 'select id,Name from Profile where Name = \'Platform Inventor\'';
        //string query1 ='select id,Name from Profile where Name = \'Platform Inventor\' limit 0';
        //system.debug('query is: '+query);
        if (Schema.sObjectType.Profile.isAccessible())
            return Database.getQueryLocator(query); else
                return null;
    }

    global void execute(Database.BatchableContext BC, List<sObject> scope) {
        List<Profile> lst = new List<Profile>();
        List<String> lstProfNames = new List<String>();
        for (Sobject s : scope) {
            Profile p = (Profile) s;
            lst.add(p);
            lstProfNames.add(p.Name);
        }
        deactivatedUsersFinal = new List<User>();
        List<User> deativatedUsers = New List<User>();
        Map<id, integer> dayslastLoginval = New Map<id, integer>();
        Integer usageThreshold = 0;
        Map<String, Licence_Recycling_JIT__c> customsetMap = Licence_Recycling_JIT__c.getAll();
        if (test.isRunningTest())
            usageThreshold = (20 * 30) / 100;
        Integer usedLicences = 0;
        Integer criteria = 0;
        Licence_Recycling_JIT__c instance ;
        if (test.isRunningTest()) {
            Licence_Recycling_JIT__c setting = new Licence_Recycling_JIT__c();
            setting.Name = 'Platform Inventor';
            setting.Licence_Type__c = 'Salesforce Platform';
            setting.Number_of_Available_Licenses__c = 10;
            setting.Deactivation_Criteria__c = 2;
            setting.Re_order_email__c = 'test@test.com';
            setting.Reorder_point__c = 45;
            setting.Reorder_Quantity__c = 200;
            setting.Total_Number_of_Licences__c = 20;
            setting.Used_Licenses__c = 10.0;
            setting.Usage_Recycling__c = 40;
            instance = setting;
        }
        if (!test.isRunningTest()) {
            usageThreshold = Integer.valueOf((Licence_Recycling_JIT__c.getInstance('Platform Inventor').Total_Number_of_Licences__c * Licence_Recycling_JIT__c.getInstance('Platform Inventor').Usage_Recycling__c) / 100);
            usedLicences = Integer.valueof(Licence_Recycling_JIT__c.getInstance('Platform Inventor').Used_Licenses__c);
            criteria = Integer.valueof(Licence_Recycling_JIT__c.getInstance('Platform Inventor').Deactivation_Criteria__c);
            instance = Licence_Recycling_JIT__c.getInstance('Platform Inventor');
        }


        if (Schema.sObjectType.User.isAccessible()) {
            List<user> loginHistory = [SELECT id,Name,Email,Username,isActive,Profile.Name, LastLogindate FROM User WHERE isActive = true and LastLogindate != null And Profile.Name = :lstProfNames];//:ProfileVal
            system.debug('loginHistory: ' + loginHistory);
            for (user u : loginHistory) {
                ProfileVal = u.Profile.Name;
                if (!customsetMap.isEmpty() && ProfileVal == 'Platform Inventor') {
                    //List of user who are inactive since few days(get deactivation criteria from custom setting)

                    Integer diff = u.LastLogindate.Date().daysBetween(datetime.now().Date());

                    //system.debug('diff: '+diff+'!usrIds.contains(u.id): '+!usrIds.contains(u.id)+'usedLicences >= usageThreshold: '+ usedLicences + usageThreshold);
                    /*if(!test.isRunningTest())
                    {
                        User us = new User(ProfileId = [SELECT Id FROM Profile WHERE Name = 'Platform Inventor'].Id,
                                           LastName = 'name',
                                           Email = 'usertestmail@testdata.com',
                                           Username = 'usertestmail1@testdata.com' + System.currentTimeMillis(),
                                           CompanyName = 'TEST',
                                           Title = 'title',
                                           Alias = 'alias',
                                           TimeZoneSidKey = 'America/Los_Angeles',
                                           EmailEncodingKey = 'UTF-8',
                                           LanguageLocaleKey = 'en_US',
                                           LocaleSidKey = 'en_US');
                    }*/

                    if (diff >= criteria && (!usrIds.contains(u.id)) && usedLicences >= usageThreshold) {
                        //system.debug('Name is'+u.Name);
                        u.isActive = false;
                        u.UserPermissionsSFContentUser = false;
                        //u.UserPreferencesContentNoEmail = false;
                        //u.UserPreferencesContentEmailAsAndWhen = false;
                        // u.Activation_Status__c = false;
                        dayslastLoginval.put(u.id, u.LastLogindate.Date().daysBetween(datetime.now().Date()));
                        deactivatedUsersFinal.add(u);
                        system.debug('deativatedUsers: ' + deactivatedUsersFinal);
                    }
                }
            }
            if (Schema.sObjectType.User.isUpdateable() && (!test.isRunningTest()))
                update deactivatedUsersFinal;

            //get custom setting and update according to letest user recycling criteria

            if (test.isRunningTest()) {
                Licence_Recycling_JIT__c setting = new Licence_Recycling_JIT__c();
                setting.Name = 'Platform Inventor';
                setting.Licence_Type__c = 'Salesforce Platform';
                setting.Number_of_Available_Licenses__c = 10;
                setting.Deactivation_Criteria__c = 2;
                setting.Re_order_email__c = 'test@test.com';
                setting.Reorder_point__c = 45;
                setting.Reorder_Quantity__c = 200;
                setting.Total_Number_of_Licences__c = 20;
                setting.Used_Licenses__c = 10.0;
                setting.Usage_Recycling__c = 40;
                instance = setting;
            }
            //system.debug('Value for Custom setting Value : '+instance);

            if (Schema.sObjectType.UserLicense.isAccessible()) {
                UserLicense UserLicenseDetails = [select Id, Name, UsedLicenses, TotalLicenses, Status, MasterLabel, LicenseDefinitionKey from UserLicense where name = :instance.Licence_Type__c];

                //system.debug('Licence Details : '+UserLicenseDetails);
                //system.debug('Value for updated custom setting : '+instance);

                decimal reorderPoint = (instance.Reorder_point__c * instance.Total_Number_of_Licences__c) / 100;
                //system.debug('reorderPoint is: '+reorderPoint);
                //system.debug('htmlbody  : '+htmlBodyReorder);
            }
        }
    }

