    global licenceRecyclingReorderOpBatch(String listProfle, List<User> usrList, Set<Id> userIDs, Map<id, integer> dayslastLoginvalMap, Boolean isInsrt) {
        insrt = isInsrt;
        lstProfileVal = listProfle;
        deativatedUsers = usrList;
        usrIds = userIDs;
        //sendReorder = reOrder;
        dayslastLoginval = dayslastLoginvalMap;
        //system.debug('sendReorder is: '+sendReorder);
