                if (!addedIDFs.contains(shareRecord.ParentId) && !usersAdded.contains(shareRecord.UserOrGroupId)) {
                    shareRecords.add(shareRecord);
                }
            }
        }

        if (!shareRecords.isEmpty()) {
            insert shareRecords;
        }
    }

