public with sharing class DocketingActivityController {

    @InvocableMethod(Label='Were docket activity closed on expiry' Description='Check if docket activity were closed on expiry')
    public static List<Boolean> checkIfWereClosedOnExpiry(List<Requests> dockets) {
        List<SymphonyIPM__Docketing_Activity_v2__c> activity = [SELECT Id, Invention_Disclosure__r.SymphonyIPM__Related_Patent__r.SymphonyIPM__Expiry_Date__c FROM SymphonyIPM__Docketing_Activity_v2__c WHERE Id = : dockets[0].docketingId];

        return new List<Boolean> {activity.size() > 0 && activity[0].Invention_Disclosure__r.SymphonyIPM__Related_Patent__r.SymphonyIPM__Expiry_Date__c < Date.today()};
    }

    public class Requests {
        @InvocableVariable
        public String docketingId;
    }

