public with sharing class SendPurgingEmailBatch implements Database.Batchable<SObject> {

    public Iterable<SObject> start(Database.BatchableContext param1) {
        Date deadline = PatentService.getDeadlineFromToday('Send_Purging_Email_Deadline');

        return [
                SELECT Id, Person__r.Name, Person__r.SymphonyIPM__User__r.Email,
                        Patent__c, Patent__r.Name, Patent__r.SymphonyIPM__Title_of_Invention__c
                FROM Patent_Role__c
                WHERE Patent__r.Litigation_on_hold__c = FALSE
                AND Patent__r.SymphonyIPM__Application_Date__c = :deadline
        ];
    }

    public void execute(Database.BatchableContext param1, List<SObject> patentRoles) {
        Map<Id, List<Patent_Role__c>> patentRolesMap = new Map<Id, List<Patent_Role__c>>();

        for (Patent_Role__c patentRole : (List<Patent_Role__c>)patentRoles) {
            List<Patent_Role__c> currentRoles = new List<Patent_Role__c>();
            if (patentRolesMap.containsKey(patentRole.Patent__c)) {
                currentRoles.addAll(patentRolesMap.get(patentRole.Patent__c));

            }
            currentRoles.add(patentRole);
            patentRolesMap.put(patentRole.Patent__c, currentRoles);
        }

        List<Messaging.SingleEmailMessage> emails = new List<Messaging.SingleEmailMessage>();
        for (Id patentId : patentRolesMap.keySet()) {
            for (Patent_Role__c role : patentRolesMap.get(patentId)) {
                if (role.Person__r.SymphonyIPM__User__r.Email != null && role.Person__r.SymphonyIPM__User__r.Email != '') {
                    List<String> to = new List<String>();

                    to.add(role.Person__r.SymphonyIPM__User__r.Email);
                    Messaging.SingleEmailMessage email = new Messaging.SingleEmailMessage();

                    email.setToAddresses(to);
                    email.setSubject('Documents purging notification');
                    email.setHtmlBody('Hi ' + role.Person__r.Name
                            + '<br/> Docket No - ' + role.Patent__r.Name
                            + '<br/> Title - ' + role.Patent__r.SymphonyIPM__Title_of_Invention__c
                            + '<br/> This is to inform you that the documents <i>(Initial and intermediate versions)</i> related to this application will be purged in the next 10 days from today.<br/><br/>Regards<br/> Thermo Fisher');
                    emails.add(email);
                }
            }
        }

        try {
            Messaging.sendEmail(emails);
        } catch (Exception e) {
        }
    }

    public void finish(Database.BatchableContext param1) {
        Database.executeBatch(new SendIFWPurgingEmailsBatch(), 100);
    }

