public with sharing class NOACheckList
{
    @InvocableMethod
    public static void GenerateNOAChecklist(list<Id> LstPatIds)
    {
        Id patId = LstPatIds[0];
        CreateNOADocument(patId);
    }
    @future(callout=true)
    public static void CreateNOADocument(Id patId)
    {
        SymphonyIPM__Patent__c pat = [SELECT Id,name FROM SymphonyIPM__Patent__c where Id =:patId];        
           Blob body;
           Id conDoc;
           PageReference pdf = Page.NOA_Page; 
           pdf.getParameters().put('id',patId);
           if(Test.isRunningTest()) {
               body = blob.valueOf('Unit.Test');
           } else {
               body = pdf.getContentAsPDF();
           }
      
       try
        {
            if(Schema.sObjectType.ContentVersion.isCreateable() 
                && ContentVersion.sObjectType.getDescribe().isCreateable() 
                && Schema.sObjectType.ContentVersion.isAccessible() 
                && Schema.sObjectType.ContentDocumentLink.isCreateable() 
                && ContentDocumentLink.sObjectType.getDescribe().isCreateable()
                )
            {
                ContentVersion conVer = new ContentVersion();
                conVer.ContentLocation = 'S'; // S specify this document is in SF, use E for external files
                conVer.PathOnClient = 'checklist - '+ pat.name+ '.pdf';  // The files name, extension is very important here which will help the file in preview.
                conVer.Title = 'checklist  -'+ pat.name+ '.pdf'; // Display name of the files
                conVer.VersionData =  body; // converting your binary string to Blog
                //conVer.PublishStatus = 'P';
                insert conVer;
                
                // First get the content document Id from ContentVersion
                conDoc = [SELECT ContentDocumentId FROM ContentVersion WHERE Id =:conVer.Id].ContentDocumentId;
                
                ContentDocumentLink cDe = new ContentDocumentLink();
                cDe.ContentDocumentId = conDoc;
                cDe.LinkedEntityId = pat.Id; // you can use objectId,GroupId etc
                cDe.ShareType = 'I'; // Inferred permission, checkout description of ContentDocumentLink object for more details
               // cDe.Visibility = 'ALL';
                insert cDe;
             }
             
          }
          catch(exception e){}

    }

