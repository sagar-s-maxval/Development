public with sharing class InventionDisclosureService {

    public static void sendFileEmails(Map<Id, Id> idfToCDMap) {
        Map<Id, SymphonyIPM__Additional_Inventor__c> idfMap= new Map<Id, SymphonyIPM__Additional_Inventor__c>();
        for (SymphonyIPM__Additional_Inventor__c idInventor :
            [SELECT Id, SymphonyIPM__Inventor_Email_Id__c, SymphonyIPM__Inventor_Name__c, SymphonyIPM__Invention_Disclosure_New__c, SymphonyIPM__Invention_Disclosure_New__r.SymphonyIPM__Title__c FROM SymphonyIPM__Additional_Inventor__c WHERE SymphonyIPM__Invention_Disclosure_New__c IN :idfToCDMap.keySet()]) {
            idfMap.put(idInventor.SymphonyIPM__Invention_Disclosure_New__c, idInventor);
        }

        List<Messaging.SingleEmailMessage> emailsToSend = new List<Messaging.SingleEmailMessage>();
        for (Id idfId : idfToCDMap.keySet()) {
            emailsToSend.add(formEmail(idfMap.get(idfId), idfToCDMap.get(idfId)));
        }

        Messaging.sendEmail(emailsToSend);
    }

    public static Messaging.SingleEmailMessage formEmail(SymphonyIPM__Additional_Inventor__c inventor, Id cdlId) {
        List<String> to = new List<String>();

        to.add(inventor.SymphonyIPM__Inventor_Email_Id__c);
        Messaging.SingleEmailMessage email = new Messaging.SingleEmailMessage();

        email.setToAddresses(to);
        email.setSubject('Invention disclosure ' + inventor.SymphonyIPM__Invention_Disclosure_New__r.Name + ' has been successfully ' + inventor.SymphonyIPM__Invention_Disclosure_New__r.SymphonyIPM__Disclosure_Status__c);
        email.setHtmlBody('<p>Dear ' + inventor.SymphonyIPM__Inventor_Name__c + '</p>' +
                '<br/>' +
                '<br/>' +
                '<br/>' +
                '<p>The Invention Disclosure <a href="">' + inventor.SymphonyIPM__Invention_Disclosure_New__r.Name + ' ' + inventor.SymphonyIPM__Invention_Disclosure_New__r.SymphonyIPM__Title__c + '</a> has been successfully submitted.</p>' +
                '<br/>' +
                '<br/>' +
                '<br/>' +
                '<p> Kind Regards, </p>' +
                'Thermo Fisher');

        List<ContentVersion> contentVersions = [SELECT VersionData, Title, FileType FROM ContentVersion WHERE ContentDocumentId = :cdlId AND IsLatest = TRUE];

        Messaging.EmailFileAttachment emailFileAttachment = new Messaging.EmailFileAttachment();
        emailFileAttachment.setFileName(contentVersions[0].Title + '.' + contentVersions[0].FileType);
        emailFileAttachment.setBody(contentVersions[0].VersionData);

        email.setFileAttachments(new List<Messaging.EmailFileAttachment>{
                emailFileAttachment
        });

        return email;
    }

