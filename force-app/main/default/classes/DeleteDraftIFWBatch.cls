public with sharing class DeleteDraftIFWBatch implements Database.Batchable<SObject> {

    public Iterable<SObject> start(Database.BatchableContext param1) {
        Date deadline = PatentService.getDeadlineFromToday('Delete_IFW_Deadline');
        return [
                SELECT Id
                FROM SymphonyIPM__IFW_Data_v1__c
                WHERE SymphonyIPM__Response_Status__c = 'Draft'
                AND SymphonyIPM__Asset__r.Litigation_on_hold__c = FALSE
                AND SymphonyIPM__Asset__r.SymphonyIPM__Issue_Date_of_Patent__c <= :deadline
        ];
    }

    public void execute(Database.BatchableContext param1, List<SObject> docsToDelete) {
        Database.delete(docsToDelete);
    }

    public void finish(Database.BatchableContext param1) {
    }

