    public static void updatePermissionFields(List<Invention_Disclosure_Role__c> idfRoles){
        Set<Id> inventionDisclosureIds = new Set<Id>();
        for(Invention_Disclosure_Role__c idr: idfRoles){
            inventionDisclosureIds.add(idr.Invention_Disclosure__c);
        }

        Map<String, List<String>> mapIdRoleToUserIds = new Map<String, List<String>>();
         Map<String, List<String>> mapIdRolePersonnames = new Map<String, List<String>>();
        List<Invention_Disclosure_Role__c> allInventionDisRoles = [SELECT Id,Person__r.name, Invention_Disclosure__c, Person__r.SymphonyIPM__User__r.Id, Role__c FROM Invention_Disclosure_Role__c WHERE Invention_Disclosure__c IN:inventionDisclosureIds AND Role__c <> null];
        for(Invention_Disclosure_Role__c idr: allInventionDisRoles){
            String key = idr.Invention_Disclosure__c + '-' + idr.Role__c;
            List<String> temp;
              List<String> tempname;
            if(mapIdRoleToUserIds.containsKey(key)){
                temp = mapIdRoleToUserIds.get(key);
            }else{
                temp = new List<String>();
            }
            if(mapIdRolePersonnames.containsKey(key)){
                tempname = mapIdRolePersonnames.get(key);
            }else{
                tempname = new List<String>();
            }
            tempname.add(idr.Person__r.name);
            temp.add(idr.Person__r.SymphonyIPM__User__r.Id);
            mapIdRoleToUserIds.put(key, temp);
            mapIdRolePersonnames.put(key, tempname);
        }

        Map<String, SymphonyIPM__Invention_Disclosure_New__c> mapIdf = new Map<String, SymphonyIPM__Invention_Disclosure_New__c>();
        for(String key: mapIdRoleToUserIds.keySet()){
            List<String> keySplit = key.split('-');
            String idfId = keySplit[0];
            String role = keySplit[1];
            SymphonyIPM__Invention_Disclosure_New__c temp;
            if(mapIdf.containsKey(idfId)){
                temp = mapIdf.get(idfId);
            }else{
                temp = new SymphonyIPM__Invention_Disclosure_New__c(Id=idfId);
                temp.Paralegal_Ids__c = '';
                temp.IP_Paralegal_1_Ids__c = '';
                temp.IP_Associate_2_Ids__c = '';
                temp.IP_Coordinator_Ids__c = '';
                temp.IP_Associate_Ids__c = '';
                temp.IP_Committee_Member_Ids__c = '';
                temp.IP_Compensation_Manager_Ids__c = '';
                temp.IP_Responsible_Manager_Ids__c = '';
                temp.IP_Partner_Ids__c = '';
                temp.XDiv_Partner_Ids__c = '';
                temp.Docketing_Ids__c = '';
                temp.IP_Paralegal_Names__c='';
            }

            if(role == 'IP Paralegal 1'){
                temp.Paralegal_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
                temp.IP_Paralegal_Names__c = String.join(mapIdRolePersonnames.get(key), ';');
            }else if(role == 'IP Paralegal 2'){
                temp.IP_Paralegal_1_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
             }else if(role == 'IP Associate 2'){
                temp.IP_Associate_2_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
            }else if(role == 'IP Coordinator'){
                temp.IP_Coordinator_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
                temp.IP_Coordinator_Names__c = String.join(mapIdRolePersonnames.get(key), ';');
            }else if(role == 'IP Associate 1'){
                temp.IP_Associate_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
                temp.IP_Associate_Names__c = String.join(mapIdRolePersonnames.get(key), ';');
            }else if(role == 'IP Committee member'){
                temp.IP_Committee_Member_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
                
            }else if(role == 'IP Compensation Manager'){
                temp.IP_Compensation_Manager_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
                temp.IP_Compensation_Manager_Names__c = String.join(mapIdRolePersonnames.get(key), ';');
            }else if(role == 'IP Responsible Manager'){
                temp.IP_Responsible_Manager_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
                temp.IP_Responsible_Manager_Names__c = String.join(mapIdRolePersonnames.get(key), ';');
            }else if(role == 'IP Partner'){
                temp.IP_Partner_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
                temp.IP_Partner_Names__c = String.join(mapIdRolePersonnames.get(key), ';');
            }else if(role == 'XDiv Partner'){
                temp.XDiv_Partner_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
                temp.XDiv_Partner_Names__c = String.join(mapIdRolePersonnames.get(key), ';');
            }else if(role == 'Docketing'){
                temp.Docketing_Ids__c = String.join(mapIdRoleToUserIds.get(key), ';');
                temp.Docketer_Names__c = String.join(mapIdRolePersonnames.get(key), ';');
            }else if(role == 'Previous Internal IP Contact'){
                 temp.Previous_Internal_IP_Contact_Names__c = String.join(mapIdRolePersonnames.get(key), ';');
            }

            mapIdf.put(idfId, temp);
        }

        update mapIdf.values();
    }

    public static void shareRecordsBasedOnRoles(Map<Id, Invention_Disclosure_Role__c> idfRoles){
        Set<Id> idfIds = new Set<Id>();
        for(Invention_Disclosure_Role__c pr: idfRoles.values()){
            idfIds.add(pr.Invention_Disclosure__c);
        }

        removeSharingOnRoles(idfIds);
        List<Invention_Disclosure_Role__c> inventionDisclosureRoles = [SELECT Id,Person__r.is_active__c, Person__r.SymphonyIPM__User__c, Person__r.SymphonyIPM__User__r.Id, Invention_Disclosure__c, Invention_Disclosure__r.OwnerId, Role__c FROM Invention_Disclosure_Role__c WHERE Invention_Disclosure__c IN:idfIds];
        List<SymphonyIPM__Invention_Disclosure_New__share> inventionsDisclosureShares = new List<SymphonyIPM__Invention_Disclosure_New__share>();
        List<PatentSharingMatrix__mdt> axsSetting = [SELECT ShareLevel__c, Role__c FROM PatentSharingMatrix__mdt WHERE Object_API_Name__c='SymphonyIPM__Invention_Disclosure_New__c'];
        Map<String, String> roleToAccess = new Map<String, String>();
        for(PatentSharingMatrix__mdt matrix: axsSetting){
            roleToAccess.put(matrix.Role__c, matrix.ShareLevel__c);
        }
        for(Invention_Disclosure_Role__c irole: inventionDisclosureRoles){
            if(irole.Person__r.SymphonyIPM__User__c != null){
                if(axsSetting != null && axsSetting.size() > 0){
                    if(irole.Person__r.is_active__c==true && irole.Person__r.SymphonyIPM__User__r.Id != irole.Invention_Disclosure__r.OwnerId && String.isNotBlank(irole.Role__c) && roleToAccess.containsKey(irole.Role__c)){
                        SymphonyIPM__Invention_Disclosure_New__share idshare = new SymphonyIPM__Invention_Disclosure_New__share(
                            ParentId = irole.Invention_Disclosure__c,
                            AccessLevel = roleToAccess.get(irole.Role__c),
                            UserOrGroupId = irole.Person__r.SymphonyIPM__User__r.Id,
                            RowCause = Schema.SymphonyIPM__Invention_Disclosure_New__share.rowCause.Invention_Disclosure_Role__c
                        );
                        inventionsDisclosureShares.add(idshare);
                    }
                }
            }
        }

        insert inventionsDisclosureShares;
    }

    public static void removeSharingOnRoles(Set<Id> idfIds){
        List<SymphonyIPM__Invention_Disclosure_New__share> delIDFShares = [SELECT Id FROM SymphonyIPM__Invention_Disclosure_New__share WHERE ParentId IN:idfIds AND RowCause=:Schema.SymphonyIPM__Invention_Disclosure_New__share.rowCause.Invention_Disclosure_Role__c];
        if(delIDFShares != null && delIDFShares.size() > 0){
            delete delIDFShares;
        }
    }

    public static void validateRoles(List<Invention_Disclosure_Role__c> idfRoles, Map<Id, Invention_Disclosure_Role__c> oldRolesMap){
        //TODO check for existing roles and persons
        Set<Id> idfIds = new Set<Id>();
        for (Invention_Disclosure_Role__c role : idfRoles) {
            idfIds.add(role.Invention_Disclosure__c);
        }
        Map<String, Integer> rolesPersonsMap = new Map<String, Integer>();
        Set<String> existingRoles = new Set<String>();
        for (Invention_Disclosure_Role__c role : [SELECT Id, Invention_Disclosure__c, Role__c, Person__c FROM Invention_Disclosure_Role__c WHERE Invention_Disclosure__c IN :idfIds]) {
            String key = role.Invention_Disclosure__c + '-' + role.Person__c + '-' + role.Role__c;
            //String key = role.Invention_Disclosure__c + '-' + role.Person__c;
            Integer count;
            if(rolesPersonsMap.containsKey(key)){
                count = rolesPersonsMap.get(key);
            }else{
                count = 0;
            }
            count += 1;
            rolesPersonsMap.put(key, count);
            existingRoles.add(role.Role__c);
        }
        
        System.debug('---rolesPersonsMap-- ' + rolesPersonsMap);
        for (Invention_Disclosure_Role__c role : idfRoles) {
            String key = role.Invention_Disclosure__c + '-' + role.Person__c + '-' + role.Role__c;
            //String key = role.Invention_Disclosure__c + '-' + role.Person__c;
            if(rolesPersonsMap.containsKey(key) && rolesPersonsMap.get(key) > 0){
                role.addError('Person should be unique within invention disclosure object for the role');
            } else if (role.Role__c != 'IP Committee Member' && existingRoles.contains(role.Role__c) && role.Role__c != oldRolesMap?.get(role.Id)?.Role__c){
                role.addError(role.Role__c + ' can be presented only once in invention disclosure record.');
            }
        }
    }

