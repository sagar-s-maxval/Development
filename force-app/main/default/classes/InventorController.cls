                }
                objOption.value = objInventors.Id;
                objOption.fallbackIconName = 'standard:user';
                objOption.type = 'avatar';
                objOption.variant = 'circle';
                objOptions.add(objOption);
            }
        }
        return objOptions;
    }

    @AuraEnabled
    public static List<InventorDocsWrapper> getEligibleIDInventors(Id idfId) {
        List<InventorDocsWrapper> inventors = new List<InventorDocsWrapper>();
        for (SymphonyIPM__Additional_Inventor__c inventor : [
                SELECT Id, SymphonyIPM__Inventor_Name__c, SymphonyIPM__Inventor_Email_Id__c
                FROM SymphonyIPM__Additional_Inventor__c
                WHERE SymphonyIPM__Invention_Disclosure_New__c = :idfId
                AND Is_Contribution_Applicable__c = TRUE
        ]) {
            InventorDocsWrapper wrapper = new InventorDocsWrapper();
            wrapper.Id = inventor.Id;
            wrapper.InventorName = inventor.SymphonyIPM__Inventor_Name__c;
            wrapper.InventorEmail = inventor.SymphonyIPM__Inventor_Email_Id__c;
            wrapper.agreement = '0';
            inventors.add(wrapper);
        }
        return inventors;
    }

    @AuraEnabled
    public static List<SDocsTemplateWrapper> getSDocsIDFTemplates(){
        List<SDocsTemplateWrapper> templates = new List<SDocsTemplateWrapper>();
        for(SDOC__SDTemplate__c template : [SELECT Id, Name, SDOC__Active__c, SDOC__Base_Object__c FROM SDOC__SDTemplate__c WHERE SDOC__Base_Object__c='SymphonyIPM__Additional_Inventor__c']){
            templates.add(new SDocsTemplateWrapper(template));
        }
        return templates;
    }

    @AuraEnabled
    public static void generateSDocs(String IdfRecord, List<SDocsTemplateWrapper> selectedTemplates) {
        try {

            List<Id> templateIds = new List<Id>();
            for(SDocsTemplateWrapper template: selectedTemplates){
                templateIds.add(template.Id);
            }

            String templateIdsTxt = String.join(templateIds, ',');
            List<SDOC__SDJob__c> jobs = new List<SDOC__SDJob__c>();
            for (SymphonyIPM__Additional_Inventor__c inventor : [SELECT Id, SymphonyIPM__Inventor_Name__c, SymphonyIPM__Inventor_Email_Id__c FROM SymphonyIPM__Additional_Inventor__c WHERE SymphonyIPM__Invention_Disclosure_New__c = :IdfRecord]) {
                jobs.add(new SDOC__SDJob__c(
                        SDOC__Start__c = true,
                        SDOC__Oid__c = inventor.Id,
                        SDOC__ObjApiName__c = 'SymphonyIPM__Additional_Inventor__c',
                        SDOC__SendEmail__c = '0',
                        SDOC__Doclist__c = templateIdsTxt,
                        RelatedObjectId_c__c = IdfRecord
                ));
            }
            insert jobs;
        } catch (Exception e) {
            throw e;
        }
    }

    @AuraEnabled
    public static List<ComboBoxOptionWrapper> getInventorsSDocsType() {
        List<ComboBoxOptionWrapper> options = new List<ComboBoxOptionWrapper>();
        options.add(new ComboBoxOptionWrapper('None', '0'));

        for (SDOC__SDTemplate__c template : [SELECT Id, Name FROM SDOC__SDTemplate__c WHERE SDOC__Base_Object__c = 'SymphonyIPM__Additional_Inventor__c']) {
            options.add(new ComboBoxOptionWrapper(template.Name, template.Id));
        }
        return options;
    }

    public static String getTemplatePreviewLink(Id tempId) {
        PageReference pref = new PageReference('/apex/SDOC__SDTemplateEditor');
        pref.getParameters().put('Id', tempId);
        pref.getParameters().put('Field', 'Template_XML__c');
        return pref.getUrl();
    }

    public class InventorDocsWrapper {
        @AuraEnabled
        public String Id { get; set; }

        @AuraEnabled
        public String InventorName { get; set; }

        @AuraEnabled
        public String InventorEmail { get; set; }

        @AuraEnabled
        public String agreement { get; set; }
    }

    public class SDocsTemplateWrapper{
        @AuraEnabled
        public String Id { get; set; }
        
        @AuraEnabled
        public String name { get; set; }
        
        @AuraEnabled
        public String link { get; set; }

        public SDocsTemplateWrapper(){}
        public SDocsTemplateWrapper(SDOC__SDTemplate__c template){
            this.Id = template.Id;
            this.name = template.Name;
            this.link = getTemplatePreviewLink(template.Id);
        }
    }

