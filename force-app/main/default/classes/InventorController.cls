public with sharing class InventorController {

    @AuraEnabled
    public static Boolean checkIfCurrentUserCanDeleteIdf(String idfId) {
        return [SELECT RecordId, HasDeleteAccess FROM UserRecordAccess WHERE UserId = :UserInfo.getUserId() AND RecordId = :idfId]?.HasDeleteAccess;
    }

    @AuraEnabled
    public static List<ComboBoxOptionWrapper> searchCurrentInventor(String currentUserEmail) {
        List<ComboBoxOptionWrapper> objOptions = new List<ComboBoxOptionWrapper>();
        if (Schema.SObjectType.SymphonyIPM__Inventor__c.isAccessible() && SymphonyIPM__Inventor__c.SObjectType.getDescribe().isAccessible()) {
            List<SymphonyIPM__Inventor__c> lstInventors = [SELECT Id, Name,SymphonyIPM__Email__c, SymphonyIPM__RecordTypeName__c FROM SymphonyIPM__Inventor__c WHERE SymphonyIPM__Email__c = :currentUserEmail LIMIT 1];
            for (SymphonyIPM__Inventor__c objInventors : lstInventors) {
                ComboBoxOptionWrapper objOption = new ComboBoxOptionWrapper();
                if (objInventors.SymphonyIPM__Email__c != null) {
                    objOption.label = objInventors.Name + ' (' + objInventors.SymphonyIPM__Email__c + ')';
                } else {
                    objOption.label = objInventors.Name;
                }
                objOption.value = objInventors.Id;
                objOption.fallbackIconName = 'standard:user';
                objOption.variant = 'circle';
                objOption.type = 'avatar';
                objOptions.add(objOption);
            }
        }
        return objOptions;
    }

    @AuraEnabled
    public static List<ComboBoxOptionWrapper> searchInventors(String searchWord, Id primaryInventorId, List<Id> inventorsToExclude) {
        List<ComboBoxOptionWrapper> objOptions = new List<ComboBoxOptionWrapper>();
        String searchCondition = searchWord + '%';
        if (Schema.SObjectType.SymphonyIPM__Inventor__c.isAccessible() && SymphonyIPM__Inventor__c.SObjectType.getDescribe().isAccessible()) {
            List<SymphonyIPM__Inventor__c> lstInventors = [SELECT Id, Name, SymphonyIPM__Email__c, SymphonyIPM__RecordTypeName__c FROM SymphonyIPM__Inventor__c WHERE (Name LIKE :searchCondition OR SymphonyIPM__FirstName__c LIKE :searchCondition OR SymphonyIPM__LastName__c LIKE :searchCondition OR SymphonyIPM__Email__c LIKE :searchCondition) AND Id NOT IN :inventorsToExclude AND Id != :primaryInventorId AND SymphonyIPM__Employment_status__c = 'Active' ORDER BY Name LIMIT 100];
            for (SymphonyIPM__Inventor__c objInventors : lstInventors) {
                ComboBoxOptionWrapper objOption = new ComboBoxOptionWrapper();
                if (objInventors.SymphonyIPM__Email__c != null) {
                    objOption.label = objInventors.Name + ' (' + objInventors.SymphonyIPM__Email__c + ')';
                } else {
                    objOption.label = objInventors.Name;
                }
                objOption.value = objInventors.Id;
                objOption.fallbackIconName = 'standard:user';
                objOption.variant = 'circle';
                objOption.type = 'avatar';
                objOptions.add(objOption);
            }
        }
        return objOptions;
    }

    @AuraEnabled
    public static List<ComboBoxOptionWrapper> getInventors(List<Id> inventorsIDs) {
        List<ComboBoxOptionWrapper> objOptions = new List<ComboBoxOptionWrapper>();
        if (Schema.SObjectType.SymphonyIPM__Inventor__c.isAccessible() && SymphonyIPM__Inventor__c.SObjectType.getDescribe().isAccessible()) {
