public with sharing class DeletePatentDocumentsBatch implements Database.Batchable<SObject> {

    public Iterable<SObject> start(Database.BatchableContext param1) {
        Date deadline = PatentService.getDeadlineFromToday('Delete_Patent_Document_Deadline');

        return [
                SELECT Id
                FROM SymphonyIPM__Patent_Attachments__c
                WHERE SymphonyIPM__Patent__r.SymphonyIPM__Application_Date__c <= :deadline
                AND SymphonyIPM__Patent__r.Litigation_on_hold__c = FALSE
                AND (SymphonyIPM__Version__c = 'Initial' OR SymphonyIPM__Version__c = 'Intermediate')
        ];
    }

    public void execute(Database.BatchableContext param1, List<SObject> docsToDelete) {
        Database.delete(docsToDelete);
    }

    public void finish(Database.BatchableContext param1) {
        Database.executeBatch(new ExpiredFilePurgingBatch(), 100);
    }

