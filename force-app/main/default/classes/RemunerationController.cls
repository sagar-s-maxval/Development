    @AuraEnabled
    public static Boolean checkIfUserCanOpenFlow(Id idfId, Id userId) {
        List<Remuneration__c> contributions = [SELECT Id, Is_Submitted__c FROM Remuneration__c WHERE Invention_Disclosure__c = :idfId AND User__c = :userId];
        User activeUser = [SELECT Email FROM User WHERE Id = :userId LIMIT 1];
        String userEmail = activeUser.Email;
        Boolean isContributionApplicable = [SELECT Id, Is_Contribution_Applicable__c FROM SymphonyIPM__Additional_Inventor__c WHERE SymphonyIPM__Invention_Disclosure_New__c = :idfId AND SymphonyIPM__Inventor_Email_Id__c = :userEmail LIMIT 1]?.Is_Contribution_Applicable__c;
        return (contributions.isEmpty() || !contributions.get(0).Is_Submitted__c) && isContributionApplicable;
    }

