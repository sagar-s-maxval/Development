    @AuraEnabled
    public static Decimal getMaximumAllowedContribution(Id inventionDisclosureId, Id userId) {
        String userEmail = [SELECT Email FROM User WHERE Id = :userId LIMIT 1]?.Email;
        List<SymphonyIPM__Additional_Inventor__c> inventors = [SELECT Id, Contribution__c FROM SymphonyIPM__Additional_Inventor__c WHERE SymphonyIPM__Invention_Disclosure_New__c = :inventionDisclosureId AND SymphonyIPM__Inventor__r.SymphonyIPM__Email__c != :userEmail];
        Decimal currentRemuneration = 0;
        for (SymphonyIPM__Additional_Inventor__c inventor : inventors) {
            currentRemuneration += inventor.Contribution__c;
        }
        try {
            Decimal part = 100 - currentRemuneration;
            return part.setScale(2);
        } catch (Exception e) {
            return 100;
        }
    }

    @AuraEnabled
    public static List<InventorWrapper> getInventors(Id idf, String docType) {
        Map<Id, Contribution_Document__c> contributionDocuments = new Map<Id, Contribution_Document__c>([SELECT Id, Inventor_Name__c FROM Contribution_Document__c WHERE Invention_Disclosure__c = :idf AND Document_Type__c = :docType]);
        Set<Id> documentsIdsToFilter = contributionDocuments.keySet();

        Set<Id> documentIds = new Set<Id>();
        Map<Id, Id> inventorDocumentMap = new Map<Id, Id>();
        Map<Id, ContentDocument> documentMap = new Map<Id, ContentDocument>();
        if (documentsIdsToFilter.size() > 0) {
            List<ContentDocumentLink> cdl = [SELECT Id, LinkedEntityId, ContentDocumentId FROM ContentDocumentLink WHERE LinkedEntityId IN :documentsIdsToFilter];
            for (ContentDocumentLink cdLink : cdl) {
                documentIds.add(cdLink.ContentDocumentId);
                inventorDocumentMap.put(cdLink.LinkedEntityId, cdLink.ContentDocumentId);
            }

            documentMap = new Map<Id, ContentDocument>([SELECT Id, Title, FileType, ContentSize, Description FROM ContentDocument WHERE Id IN:documentIds]);
        }

        List<InventorWrapper> inventors = new List<InventorWrapper>();
        for (Contribution_Document__c contributionDocument : contributionDocuments.values()) {
            InventorWrapper wrapper = new InventorWrapper();
            wrapper.contributionDocumentId = contributionDocument.Id;
            wrapper.inventorName = contributionDocument.Inventor_Name__c;
            wrapper.documentId = inventorDocumentMap.get(contributionDocument.Id);
            wrapper.documentName = documentMap.get(wrapper.documentId)?.Title;
            inventors.add(wrapper);
        }

        return inventors;
    }

