    @AuraEnabled
    public static void updateContribution(Id idf, Id userId, Decimal contribution) {

        String userEmail = [SELECT Email FROM User WHERE Id = :userId LIMIT 1]?.Email;
        try {
            SymphonyIPM__Additional_Inventor__c idInventor = [
                    SELECT Id
                    FROM SymphonyIPM__Additional_Inventor__c
                    WHERE SymphonyIPM__Inventor__r.SymphonyIPM__Email__c = :userEmail
                    AND SymphonyIPM__Invention_Disclosure_New__c = :idf
            ];

            idInventor.Contribution__c = contribution;
            update idInventor;

            List<Remuneration__c> remunerations = [
                    SELECT Id
                    FROM Remuneration__c
                    WHERE Invention_Disclosure__c = :idf
                    AND User__c = :userId
            ];
            if (remunerations.size() > 0) {
                remunerations[0].Contribution__c = contribution;
                update remunerations;
            }

        } catch (Exception exp) {
            throw exp;
        }
    }

    @AuraEnabled
    public static void updateSplitContribution(Id idf, Decimal contribution) {
        try {
            List<SymphonyIPM__Additional_Inventor__c> inventors = [
                    SELECT Id
                    FROM SymphonyIPM__Additional_Inventor__c
                    WHERE SymphonyIPM__Invention_Disclosure_New__c = :idf
            ];

            for (SymphonyIPM__Additional_Inventor__c idInventor : inventors) {
                idInventor.Contribution__c = contribution;
            }

            Database.update(inventors);

            List<Remuneration__c> remunerations = [
                    SELECT Id
                    FROM Remuneration__c
                    WHERE Invention_Disclosure__c = :idf
            ];
            for (Remuneration__c remuneration : remunerations) {
                remuneration.Contribution__c = contribution;
            }
            Database.update(remunerations);

        } catch (Exception exp) {
            throw exp;
        }
    }

