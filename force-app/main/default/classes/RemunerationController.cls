    @AuraEnabled
    public static Decimal countRemuneration(Id inventionDisclosureId) {
        List<SymphonyIPM__Additional_Inventor__c> inventors = [SELECT Id, Contribution__c FROM SymphonyIPM__Additional_Inventor__c WHERE SymphonyIPM__Invention_Disclosure_New__c = :inventionDisclosureId];

