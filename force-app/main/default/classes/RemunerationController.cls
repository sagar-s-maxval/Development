            FROM SymphonyIPM__Invention_Disclosure_New__c
            WHERE Id = :inventionDisclosureId]?.Is_Contribution_Accepted__c;
        
        if(checkIfUserHasEditAccess(inventionDisclosureId) && !hasAccess){
