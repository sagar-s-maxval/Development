    @InvocableMethod(Label='Create 22 docketing activities' Description='If the invention claim is yes and invention is in use')
    public static void create22DocketingActivities(List<Requests> idfs) {
        List<SymphonyIPM__Docketing_Activity_v2__c> activities = new List<SymphonyIPM__Docketing_Activity_v2__c>();

        for (Requests idf : idfs) {
            for (Integer i = 1; i <= 22; i++) {
                activities.add(new SymphonyIPM__Docketing_Activity_v2__c(
                        Invention_Disclosure__c = idf.inventionDisclosureId,
                        SymphonyIPM__Due_Date__c = idf.claimDate.addYears(i),
                        SymphonyIPM__Activity_Date__c = idf.claimDate,
                        Is_Flat_Fee__c = idf.isFlatFee,
                        SymphonyIPM__Actions__c = i == 1 ?
                                'Utilization (1st anniversary)' : i == 2 ?
                                        'Utilization (2nd anniversary)' : i == 3 ?
                                                'Utilization (3rd anniversary)' : i == 21 ?
                                                        'Utilization (21st anniversary)' : i == 22 ?
                                                                'Utilization (22nd anniversary)' : 'Utilization (' + i + 'th anniversary)'
                ));
            }
        }

        insert activities;
    }

    private static Boolean checkIfUserHasEditAccess(String idfId){
        Profile sysAdminProfile = [SELECT Id, Name FROM Profile WHERE Id=:UserInfo.getProfileId()];
        if(sysAdminProfile.Name == 'System Administrator'){
            return true;
        }
        Boolean access = false;
        List<Invention_Disclosure_Role__c> userCurrentRoles = [SELECT Id, Role__c FROM Invention_Disclosure_Role__c WHERE Invention_Disclosure__c=:idfId AND Person__r.SymphonyIPM__User__c=:UserInfo.getUserId()];
        Set<String> roles = new Set<String>();
        if(userCurrentRoles != null && userCurrentRoles.size() > 0){
            for(Invention_Disclosure_Role__c role: userCurrentRoles){
                roles.add(role.Role__c);
            }

            List<PatentSharingMatrix__mdt> roleMatrix = [SELECT ShareLevel__c FROM PatentSharingMatrix__mdt WHERE Object_API_Name__c = 'SymphonyIPM__Invention_Disclosure_New__c' AND Role__c IN:roles ORDER BY ShareLevel__c DESC];
            Set<String> accessLevels = new Set<String>();
            for(PatentSharingMatrix__mdt matrix: roleMatrix){
                accessLevels.add(matrix.ShareLevel__c);
            }

            if(accessLevels.contains('edit')){
                access = true;
            }
        }

        return access;
    }

