public class IDFPDFController{

    public IDFPDFController() {
        recordid =ApexPages.currentpage().getParameters().get('Id');
    }


    public String richtextValue='';
    public String recordid='';
    public String fieldName='Known_relevant_existing_solution_s__c';
    public SymphonyIPM__IDF_Questions__c settings {get;set;}
    public SymphonyIPM__Invention_Disclosure_New__c Invention_Disclosure {get;set;}
    public String LstInventors {get;set;}
    public String relatedInvDisclosures {get;set;}
    public String country {get;set;}
    public String path {get;set;}
    public string inventor {get;set;}
    public string disclosureStatus {get;set;}
    

    public  IDFPDFController(ApexPages.StandardController controller) {
        SymphonyIPM__Invention_Disclosure_New__c disc= (SymphonyIPM__Invention_Disclosure_New__c)controller.getRecord();
        disc.Id =(Id)ApexPages.currentpage().getParameters().get('Id');
        //recotrdID =disc.Id;
        SymphonyIPM__Invention_Disclosure_New__c Invention_Disclosure=getData(disc.Id);
        inventor =Invention_Disclosure.SymphonyIPM__Primary_Inventor__r.Name;  
        for (SymphonyIPM__Additional_Inventor__c adinv: Invention_Disclosure.SymphonyIPM__Inventor_Details__r) {                   
            if (LstInventors == '' || LstInventors == null){
                LstInventors = adinv.SymphonyIPM__Inventor__r.Name;                   
            }
            else {
                LstInventors = LstInventors + ', ' + adinv.SymphonyIPM__Inventor__r.Name;                     
            }
        }
        //Sobject record=Database.query('Select Id,' + fieldName +' from SymphonyIPM__Invention_Disclosure_New__c Where Id =:recotrdID ');
        //richtextValue =String.valueOf(record.get(fieldName));
        settings = SymphonyIPM__IDF_Questions__c.getInstance('Invention Disclosure Questions');
        disclosureStatus =String.valueOf(Invention_Disclosure.get(settings.SymphonyIPM__Namespace__c + 'Disclosure_Status__c'));
    }
    
    public SymphonyIPM__Invention_Disclosure_New__c getData(Id recordId){
        return [Select Id,LastModifiedDate,SymphonyIPM__Title__c,SymphonyIPM__Inventor_Email_Id__c,SymphonyIPM__Invention_Date__c,SymphonyIPM__Invention_Country__c,SymphonyIPM__Invention_Country__r.Name,SymphonyIPM__PPA_EmailId__c,
                        SymphonyIPM__Detailed_Description__c,SymphonyIPM__Inventive_Features__c,SymphonyIPM__Invention_Advantages__c,SymphonyIPM__Alternative_Implementations_New__c,
                        SymphonyIPM__Made_or_Used__c,SymphonyIPM__Made_or_Used_Date__c,SymphonyIPM__Sold__c,SymphonyIPM__Sale_Date__c,SymphonyIPM__Export_Control__c,SymphonyIPM__Open_Source__c,SymphonyIPM__Hierarchy__c,SymphonyIPM__Hierarchy__r.Name,
                        SymphonyIPM__Disclosure_Status__c,(select SymphonyIPM__Inventor__c,SymphonyIPM__Inventor__r.Name from SymphonyIPM__Inventor_Details__r)
                        from SymphonyIPM__Invention_Disclosure_New__c where Id =: recordId];
    }

