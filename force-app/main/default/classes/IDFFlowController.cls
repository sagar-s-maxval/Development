    @AuraEnabled
    public static Boolean checkIfUserHasEditAccess(Id recordId){
        List<Profile> inventorOrAdmin = [SELECT Id, Name FROM Profile WHERE Id=:UserInfo.getProfileId() AND Name IN ('System Administrator', 'Platform Inventor', 'In-house Counsel')];
        if(inventorOrAdmin != null && inventorOrAdmin.size() > 0){
            return true;
        }
        
        if(String.isNotBlank(recordId)){
            List<Invention_Disclosure_Role__c> idfRoles = [SELECT Id, Role__c FROM Invention_Disclosure_Role__c WHERE Invention_Disclosure__c =:recordId AND Person__r.SymphonyIPM__User__r.Id =:UserInfo.getUserId()];
            if(idfRoles != null && idfRoles.size() > 0){
                Set<String> rolesHasEditAccess = new Set<String>{'IP Coordinator','IP Responsible Manager', 'IP Paralegal 1', 'IP Partner', 'XDiv Partner', 'Docketing'};
                if(rolesHasEditAccess.contains(idfRoles[0].Role__c)){
                    return true;        
                }
            }   
        }else{
            List<Profile> icProfile = [SELECT Id, Name FROM Profile WHERE Id=:UserInfo.getProfileId() AND Name IN ('In-house Counsel')];
            if(icProfile != null && icProfile.size() > 0){
                return true;
            }
        }
        
        return false;
    }
    
