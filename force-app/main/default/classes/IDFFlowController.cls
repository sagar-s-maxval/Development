    @AuraEnabled
    public static List<SymphonyIPM__Inventor__c> getAdditionalInventors(Id idf, Id primaryInventorId) {
        List<SymphonyIPM__Additional_Inventor__c> idInventors = [
                SELECT Id, SymphonyIPM__Inventor__c
                FROM SymphonyIPM__Additional_Inventor__c
                WHERE SymphonyIPM__Invention_Disclosure_New__c = :idf
                AND Is_External_Inventor_New__c = FALSE
        ];

        Set<Id> ids = new Set<Id>();
        for (SymphonyIPM__Additional_Inventor__c inventor : idInventors) {
            ids.add(inventor.SymphonyIPM__Inventor__c);
        }

        return [
                SELECT Id
                FROM SymphonyIPM__Inventor__c
                WHERE Id IN :ids AND Id != :primaryInventorId
        ];
    }

