<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction,lightning:availableForFlowScreens,lightning:actionOverride,force:appHostable,lightning:isUrlAddressable" access="global">    
    
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    <aura:attribute name="HomePage" type="boolean" default="false"/>
    <aura:attribute name="refreshvarbool" type="boolean" default="False"/>
    <div class="slds-box slds-theme_default">       
        <lightning:flow aura:id="flowData" onstatuschange="{!c.handleStatusChange}"/>
    </div>
</aura:component>