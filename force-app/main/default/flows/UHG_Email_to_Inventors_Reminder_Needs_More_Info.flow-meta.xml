    <formulas>
        <name>ExpirationDateForConceptPassed</name>
        <dataType>Boolean</dataType>
        <expression>AND( 
ISPICKVAL({!$Record.Current_Stage_of_Invention__c}, &quot;Concept&quot;), 
{!$Flow.CurrentDate} &gt; ADDMONTHS({!$Record.UHG_Reminder_Date__c}, 9), 
ISBLANK({!$Record.UHG_First_External_Use_Date__c}), 
ISBLANK({!$Record.UHG_First_Use_Date__c}), 
ISBLANK({!$Record.UHG_Future_Use_Date__c}))</expression>
    </formulas>
    <formulas>
        <name>IDFHyperlink</name>
        <dataType>String</dataType>
        <expression>&quot;&lt;a href=&apos;http://uhg.my.salesforce.com/&quot;+{!$Record.Id}+&quot;&apos;&gt;&quot;+{!$Record.Name}+&quot;&lt;/a&gt;&quot;</expression>
    </formulas>
    <formulas>
        <name>MentorStatement</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.Mentors__c} &lt;&gt; &quot;&quot;, &quot;If IP Legal assigned mentors to you, please work with them (&quot; + {!$Record.Mentors__c} + &quot;) to update your IDF.&quot;, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>NickName</name>
        <dataType>String</dataType>
        <expression>IF({!Inventors.Nick_Name__c}&lt;&gt;&quot;&quot;,{!Inventors.Nick_Name__c},{!Inventors.First_Name__c})</expression>
    </formulas>
    <formulas>
        <name>TextDate</name>
        <dataType>String</dataType>
        <expression>TEXT(YEAR({!$Flow.CurrentDate})) + &apos;-&apos; + LPAD(TEXT(MONTH({!$Flow.CurrentDate})),2,&apos;0&apos;) + &apos;-&apos; + LPAD(TEXT(DAY({!$Flow.CurrentDate})),2,&apos;0&apos;)</expression>
    </formulas>
    <formulas>
        <name>Trimnames</name>
        <dataType>String</dataType>
        <expression>CASE({!AdditionalInventorsCount},
1,TRIM(SUBSTITUTE(SUBSTITUTE({!AdditionalInventorFirstNames},&quot;[&quot;,&apos; &apos;),&quot;]&quot;,&apos; &apos;)),
2,TRIM(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE({!AdditionalInventorFirstNames},&quot;[&quot;,&apos; &apos;),&quot;]&quot;,&apos; &apos;),&quot;,and ,&quot;,&quot; and &quot;),&quot;,&quot;,&quot;, &quot;)),
TRIM(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE({!AdditionalInventorFirstNames},&quot;[&quot;,&apos; &apos;),&quot;]&quot;,&apos; &apos;),&quot;and ,&quot;,&quot;and &quot;),&quot;,&quot;,&quot;, &quot;)))</expression>
    </formulas>
    <formulas>
        <name>UserFirstName</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.SymphonyIPM__Internal_Counsel__r.UHG_Preferred_First_Name__c}&lt;&gt;&quot;&quot;, {!$Record.SymphonyIPM__Internal_Counsel__r.UHG_Preferred_First_Name__c}, {!$Record.SymphonyIPM__Internal_Counsel__r.FirstName})</expression>
    </formulas>
    <formulas>
